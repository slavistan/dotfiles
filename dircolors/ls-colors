#!/bin/zsh
#
# These values are loaded into the LS_COLORS environment variable.
# The linebreaks and comments are removed beforehand.
#
# The LS_COLORS environment variable is a colon:separated list of key/color pairs.
#
cat "$0" |
    awk '!/^ *#/ { print }' | # throw away lines starting with #
    sed -re '1,/^exit 0$/ d'  | # remove these shell script lines
    sed -re 's/#.*$//g'  | # remove trailing comments
    sed -re 's/^ +//' -re 's/ +$//' | # remove leading and trailing whitespaces
    sed -re '/^$/d' | # remove blank lines
    sed -re 's/$/:/g' | # append colon
    tr -d '\n' # concatenate lines

exit 0

