mod4 + d
  dmenu_run -p "Run:"

mod4 + o
  kill -SIGUSR1 $(pgrep sxhkd)

mod4 + Return
  $TERMINAL -e sh -c "cd $(xcwd); zsh"

ctrl + shift + mod4 + q
  $DOTFILES/scripts/dwm/dwm-logout.sh

ctrl + shift + mod4 + l
  dm-tool lock

mod4 + n
  $DOTFILES/scripts/snapshot.sh --select-area

mod4 + e
  spacefm
