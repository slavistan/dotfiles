# Path to your oh-my-zsh installation.
export ZSH="/home/shuell/.config/zsh/oh-my-zsh"
export ZSH_HOME="/home/shuell/.config/zsh"

ZSH_THEME="robbyrussell"
ENABLE_CORRECTION="true"
COMPLETION_WAITING_DOTS="true"

plugins=(
  git wd colored-man-pages extract zsh-syntax-highlighting zsh-dircolors-solarized
)

source $ZSH/oh-my-zsh.sh
source $ZSH_HOME/startup.zsh
source $ZSH_HOME/functions.zsh

# User configuration

export EDITOR='vim'
export BROWSER='firefox'

alias xclip='xclip -selection clipboard'
alias vim=nvim
